{
  "name": "node-api-to-vue-spa",
  "version": "1.0.0",
  "description": "Essa API Node Ã© um exemplo",
  "main": "index.js",
  "scripts": {
    "mock": "npm-run-all dev:step:** -s",
    "dev": "npm-run-all dev:step:nomock:** -s",
    "dev:step:nomock:await-mysql": "ts-node scripts/check-mysql.ts",
    "dev:step:dropdb": "sequelize db:drop",
    "dev:step:nomock:createdb": "sequelize db:create",
    "dev:step:nomock:migration": "sequelize db:migrate",
    "dev:step:nomock:dbseed": "ts-node scripts/seed-all.ts",
    "dev:step:mock": "ts-node scripts/mock.ts",
    "dev:step:nomock:start": "nodemon index.ts",
    "dist": "npm-run-all dist:step:** -s",
    "dist:step:await-mysql": "ts-node dist/scripts/check-mysql.js",
    "dist:step:createdb": "sequelize db:create --config dist/config/config.js",
    "dist:step:migration": "sequelize db:migrate --config dist/config/config.js",
    "dist:step:dbseed": "ts-node dist/scripts/seed-all.js",
    "dist:step:start": "node dist/index.js"
  },
  "keywords": [
    "swagger"
  ],
  "license": "Unlicense",
  "private": true,
  "dependencies": {
    "bcrypt": "^5.0.1",
    "connect": "^3.2.0",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "gulp-cli": "^2.3.0",
    "helmet": "^4.6.0",
    "js-yaml": "^3.3.0",
    "jsonwebtoken": "^8.5.1",
    "moment": "^2.29.1",
    "mysql2": "^2.2.5",
    "oas3-tools": "^2.2.3",
    "pg": "^8.6.0",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.6.2"
  },
  "devDependencies": {
    "@types/node": "^15.14.1",
    "@types/validator": "^13.6.2",
    "gulp": "^4.0.2",
    "gulp-typescript": "^6.0.0-alpha.1",
    "nodemon": "^2.0.7",
    "npm-run-all": "^4.1.5",
    "sequelize-cli": "^6.3.0",
    "ts-node": "^10.0.0",
    "typescript": "^4.3.5"
  }
}
